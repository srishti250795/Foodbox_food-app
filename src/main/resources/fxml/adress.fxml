<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.mycompany.fds.Controller.AdressController">
   <children>
      <ImageView fitHeight="211.0" fitWidth="600.0" pickOnBounds="true">
         <viewport>
            <Rectangle2D height="211.0" width="600.0" />
         </viewport>
      </ImageView>
      <Label fx:id="lab" layoutX="116.0" layoutY="106.0" prefHeight="24.0" prefWidth="392.0" text="Merci de saisir votre adresse de commande" textFill="#d4413f">
         <font>
            <Font name="Segoe UI" size="20.0" />
         </font></Label>
      <Button layoutX="122.0" layoutY="328.0" mnemonicParsing="false" onAction="#btn" prefHeight="31.0" prefWidth="377.0" style="-fx-background-color: #d4413f;" text="Valider" textFill="WHITE">
         <font>
            <Font name="Segoe UI Bold" size="15.0" />
         </font></Button>
      <JFXTextField fx:id="text" alignment="CENTER" focusColor="#d4413f" labelFloat="true" layoutX="122.0" layoutY="246.0" prefHeight="50.0" prefWidth="377.0" promptText="ex: Avenue Mohamed Ben Abdellah Regragui, Ensias, Rabat">
         <font>
            <Font name="Segoe UI" size="12.0" />
         </font></JFXTextField>
   </children>
</AnchorPane>
